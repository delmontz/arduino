     if ((0xE2 <= char1) && (char1 <= 0xEF)){   // Unicode "E2xxxx"～"E9xxxx"
       char2 = *p2++; char3 = *p2++;
       switch(char1 - 0xE2) {
         case  0: buildMapCMP(font_E2, sizeof(font_E2) / sizeof(font_E2[0]), char2, char3); break;
         case  1: buildMapCMP(font_E3, sizeof(font_E3) / sizeof(font_E3[0]), char2, char3); break;
         case  2: buildMapCMP(font_E4, sizeof(font_E4) / sizeof(font_E4[0]), char2, char3); break;
         case  3: buildMapCMP(font_E5, sizeof(font_E5) / sizeof(font_E5[0]), char2, char3); break;       
         case  4: buildMapCMP(font_E6, sizeof(font_E6) / sizeof(font_E6[0]), char2, char3); break;
         case  5: buildMapCMP(font_E7, sizeof(font_E7) / sizeof(font_E7[0]), char2, char3); break;
         case  6: buildMapCMP(font_E8, sizeof(font_E8) / sizeof(font_E8[0]), char2, char3); break;
         case  7: buildMapCMP(font_E9, sizeof(font_E9) / sizeof(font_E9[0]), char2, char3); break;
         case 13: buildMapCMP(font_EF, sizeof(font_EF) / sizeof(font_EF[0]), char2, char3); break;
         default:for(int i = 2; i < sizeof(fErrMap); i++)fBitMap[i] = fErrMap[i];        
        }
        DBsimulation(fBitMap, GLCD.Coord.x, GLCD.Coord.y, BLACK);
     } else {
       if(char1 == '\n') {
         GLCD.GotoXY(x, GLCD.Coord.y+8);
       } else {
         GLCD.PutChar(char1);                // 
       }
     }
   }
 }
/**********************************
 * DrawBitmapをシミュレーション   *
 **********************************/
void DBsimulation(uint8_t* p2, uint8_t x, uint8_t y, uint8_t color){
  uint8_t width, height;
  uint8_t i, j, k;
  int bitmap = 0;
  width = p2[bitmap++]; 
  height = p2[bitmap++]; 
  for(j = 0; j < height / 8; j++) {
    for(i = 0; i < width; i++) {
      uint8_t displayData = p2[bitmap++];
      for(k = 0; k < 8; k++) {
        uint8_t displayBit = displayData & 0x01;
        if(displayBit == 1){
          GLCD.SetDot(x + i, y + (j*8 + k), color);   
        } else {
          GLCD.SetDot(x + i, y + (j*8) + k, ~color);
        }      
        displayData = displayData >> 1;
      }
    }
  }
}
/**************************************************************
 * 受信データの解析                                           *
 **************************************************************/
void analysis(uint8_t p1[], uint8_t p2[]){
  switch (p1[0]) {
    case C_ClearPage:               // *ClearPage コマンド
      GLCD.ClearPage(p1[1], p1[2]);
      break;
    case C_ClearScreen:             // *ClearScreen コマンド
      GLCD.ClearScreen(p1[1]);
      break;
    case C_CursorTo:                // *CursorTo コマンド
      GLCD.CursorTo(p1[1], p1[2]);
      break;
    case C_DrawBitmap:              // *DrawBitmap コマンド 
      DBsimulation(p2, p1[1], p1[2], p1[3]); // DrawBitmapをシミュレーション
      break;  
    case C_DrawLine:                // *DrawLine コマンド
      GLCD.DrawLine(p1[1], p1[2], p1[3], p1[4], p1[5]);  
      break;
    case C_DrawRect:                // *DrawRect コマンド
      GLCD.DrawRect(p1[1], p1[2], p1[3], p1[4], p1[5]);     
      break;      
    case C_DrawRoundRect:           // *DrawRoundRect コマンド
      GLCD.DrawRoundRect(p1[1], p1[2], p1[3], p1[4], p1[5], p1[6]);
      break;
    case C_FillRect:                // *FillRect コマンド
      GLCD.FillRect(p1[1], p1[2], p1[3], p1[4], p1[5]);
      break;      
    case C_GotoXY:                  // *GotoXY コマンド
      GLCD.GotoXY(p1[1], p1[2]);
      break;
    case C_Init:                    // *Init コマンド  z  
      Pins_Init(0,4,1,5,2,3,6,7,8,9,10,11,12,13);
      GLCD_Start();  
      GLCD.Init(p1[1]);             //********************************
      GLCD.ClearScreen();  
      GLCD.SelectFont(System5x7);   // switch to fixed width system font
      break;
    case C_InvertRect:              // *InvertRect コマンド
      GLCD.InvertRect(p1[1], p1[2], p1[3], p1[4]);
      break;
    case C_PrintNumber:             // *PrintNumber コマンド    
      GLCD.PrintNumber((p1[1] << 8) + p1[2]);       
      break;      
    case C_Puts:                    // *Puts コマンド
      kanji(p2);                    // 要漢字処理
      break;
    case C_Puts_P:                  // *Puts_P コマンド
      kanji(p2);                    // 要漢字処理
      break;     
    case C_PutChar:                 // *PutChar コマンド
      GLCD.PutChar(p1[1]);    
      break; 
    case C_SelectFont:              // *SelectFont コマンド
      for(int i = 0; i < 6; i++){
        if(pgm_read_byte_near(Arial_14 + i) != p1[i+1]) break;
        if(i = 5) GLCD.SelectFont(Arial_14);
      }
      for(int i = 0; i < 6; i++){
        if(pgm_read_byte_near(System5x7 + i) != p1[i+1]) break;
        if(i = 5) GLCD.SelectFont(System5x7);
      }      
      break;
    case C_SetDot:                 // *SetDot コマンド
      GLCD.SetDot(p1[1], p1[2], p1[3]);    
      break;  
    case C_SetInverted:            // *SetInverted コマンド  
      GLCD.SetInverted(p1[1]); 
      break;  
  } 
}
