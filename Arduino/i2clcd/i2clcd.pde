/*******************************************************************************
*  Pins_Start(rst,rs,rw,enable,cs1,cs2,d0,d1,d2,d3,d4,d5,d6,d7)                 *
*    グラフィックLCDモジュールと接続するピンの初期化処理                       *
*                                                                              *
*    rst    : RST(reset signal)信号と接続しているピン番号を指定                *
*    rs     : RS(register select)信号と接続しているピン番号を指定              *
*    rw     : RW(read/write)信号と接続しているピン番号を指定                   *
*    enable : E (enable signal)信号と接続しているピン番号を指定                *
*    cs1    : CS1(selection IC1)信号と接続しているピン番号を指定               *
*    cs2    : CS2(selection IC2)信号と接続しているピン番号を指定               *
*    d0-d7  : DB4-7(data bit)信号と接続しているピン番号を指定                  *
*******************************************************************************/
void Pins_Init(int rst,int rs,int rw,int enable,int cs1,int cs2,
               int d0,int d1,int d2,int d3,int d4,int d5,int d6,int d7)
{
     // データピンの番号をセット
     DATA_pins[0] = d0 ;
     DATA_pins[1] = d1 ;
     DATA_pins[2] = d2 ;
     DATA_pins[3] = d3 ;
     DATA_pins[4] = d4 ;
     DATA_pins[5] = d5 ;
     DATA_pins[6] = d6 ;
     DATA_pins[7] = d7 ;
     // 制御ピンの番号をセット
     RS_pin  = rs ;
     RW_pin  = rw ;
     E_pin   = enable ;
     CS1_pin = cs1 ;
     CS2_pin = cs2 ;
     // 指定の制御ピンをデジタル出力に設定
     pinMode(RS_pin, OUTPUT) ;
     pinMode(RW_pin, OUTPUT) ;
     pinMode(E_pin, OUTPUT) ;
     pinMode(CS1_pin, OUTPUT) ;
     pinMode(CS2_pin, OUTPUT) ;
     // 指定のデータピンをデジタル出力に設定
     for (int i = 0; i < 8; i++) pinMode(DATA_pins[i], OUTPUT) ;
     // 信号線をLOWに設定して置きます
     digitalWrite(RS_pin, LOW);
     digitalWrite(RW_pin, LOW);
     digitalWrite(E_pin, LOW);
     digitalWrite(CS1_pin, LOW);
     digitalWrite(CS2_pin, LOW);
     // グラフィックLCDモジュールのリセットを解除する
     pinMode(rst, OUTPUT) ;
     digitalWrite(rst, HIGH);
}
/*******************************************************************************
*  GLCD_Init()                                                                 *
*    グラフィックLCDモジュールに初期化情報を発行する処理                       *
*******************************************************************************/
void GLCD_Start()
{
     // LCD電源がONしてから30msほどウエイトするみたいなので入れておく
     delay(30) ;
     // 左側ディスプレイのチップ(IC1)を初期化する
     SelectIC(1) ;		     // IC1を選択
     command(0xC0,LOW) ;		// Display start lineのコマンドを発行する
     command(0x3F,LOW) ;		// Display Onのコマンドを発行する
     // 右側ディスプレイのチップ(IC2)を初期化する
     SelectIC(2) ;            // IC2を選択
     command(0xC0,LOW) ;		// Display start lineのコマンドを発行する
     command(0x3F,LOW) ;		// Display Onのコマンドを発行する
}
/*******************************************************************************
*  SelectIC(value)                                                             *
*    ディスプレイのチップ(IC)を選択する処理                                    *
*                                                                              *
*    value : 1=左側ディスプレイのチップ(IC1)  2=右側ディスプレイのチップ(IC2)  *
*******************************************************************************/
void SelectIC(int value)
{
     if (value==1) {
          // 左側ディスプレイのチップ(IC1)を選択する
          digitalWrite(CS1_pin, HIGH);
          digitalWrite(CS2_pin, LOW);
     } else {
          // 右側ディスプレイのチップ(IC2)を選択する
          digitalWrite(CS2_pin, HIGH);
          digitalWrite(CS1_pin, LOW);
     }
}
/*******************************************************************************
*  SetPage(value)                                                              *
*    ページの設定を行うコマンドを発行する処理                                  *
*                                                                              *
*    value : ページ０～７を選択する(縦アドレス)                                *
*******************************************************************************/
void SetPage(int value)
{
     command(0xB8|(value&0x07),LOW) ;
}
/*******************************************************************************
*  SetAddress(value)                                                           *
*    カラムアドレスの設定を行うコマンドを発行する処理                          *
*                                                                              *
*    value : ０～６３を選択する(横アドレス)                                    *
*******************************************************************************/
void SetAddress(int value)
{
     command(0x40|(value&0x3F),LOW) ;
}
/*******************************************************************************
*  ans = ReadData()                                                            *
*    現在表示されているデータの内容を読込むコマンドを発行する処理              *
*                                                                              *
*    ans : ８ビットの読込んだデータを返す                                      *
*******************************************************************************/
unsigned int ReadData()
{
     unsigned int ans , i ;

     // データピンをデジタル入力に設定
     for (i = 0; i < 8; i++) pinMode(DATA_pins[i], INPUT) ;
     // グラフィックLCDモジュールから読込みモードに設定する
     digitalWrite(RW_pin, HIGH);
     digitalWrite(RS_pin, HIGH) ;
     // データピンからデータを読込む
     digitalWrite(E_pin, HIGH) ;
     digitalWrite(E_pin, LOW) ;
     ans = 0 ;
     digitalWrite(E_pin, HIGH) ;
     for (i = 0; i < 8; i++) {
          ans = ans | (digitalRead(DATA_pins[i]) << i);
     }
     digitalWrite(E_pin, LOW) ;
     // モードを書込みに設定しデータピンをデジタル出力に戻す
     digitalWrite(RW_pin, LOW);
     for (i = 0; i < 8; i++) pinMode(DATA_pins[i], OUTPUT) ;

     return( ans ) ;
}
/*******************************************************************************
*  WriteData(value)                                                            *
*    表示するデータを書込むコマンドを発行する処理                              *
*    書込んだ後、カラムアドレスは自動的に次のアドレスへ移動します。            *
*                                                                              *
*    value : ８ビットのデータを指定する                                        *
*******************************************************************************/
void WriteData(int value)
{
     command(value,HIGH) ;
}
// グラフィックLCDモジュールコマンドを発行する処理
void command(int value,int mode)
{
     int i ;

     // RSの制御信号線をセットします
     digitalWrite(RS_pin, mode) ;
     // D0-D7のデータ線にセットします
     for (i = 0; i < 8; i++) {
          digitalWrite(DATA_pins[i], (value >> i) & 0x01) ;
     }
     // Eの制御信号線をセットします
     digitalWrite(E_pin, HIGH) ;
     digitalWrite(E_pin, LOW) ;
}
