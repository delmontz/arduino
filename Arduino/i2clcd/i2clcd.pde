 **********************************/
int pos1, pos2;
uint8_t rData1[10];                 // パラメータ格納領域
uint8_t rData2[maxBitmap+1];        // データ格納領域
void receiveEvent(int howMany){
  ready = 1;                        // データ処理開始
  while(1 < Wire.available()){      // loop through all but the last 
    uint8_t c1 = Wire.receive();    // receive byte
    uint8_t c2 = Wire.receive();    // receive byte 
    digitalWrite(ledPin, HIGH);     // 
    if (c1 == W_Cmd) {              // *Start + コマンド
      pos1 = 0; pos2 = 0;
      rData1[pos1++] = c2;
    } else if (c1 == W_Para) {      // *パラメータ
      rData1[pos1++] = c2;
    } else if (c1 == W_Data) {      // *データ
      if(pos2 < maxBitmap) {
        rData2[pos2++] = c2;
      }
    } else if (c1 == W_End ) {      // *End
        analysis(rData1, rData2);
    }
    digitalWrite(ledPin, LOW);      //   
  }
  ready = 0;                        // データ処理終了
}
